# vim: ft=gitconfig
# Scheme System: base16
# Scheme Name: Oxocarbon Dark
# Scheme Author: shaunsingh/IBM
# Template Author: Bez Hermoso

# Configures difftool "delta" to ue light or dark mode based on the variant declared by the colorscheme.
[delta]
    syntax-theme = "ansi"
    light = false
    minus-style = reverse "#3ddbd9"
    minus-emph-style = reverse "#be95ff"
    plus-style = reverse "#33b1ff"
    plus-emph-style = reverse "#be95ff"
