# vim: ft=gitconfig
# Scheme System: base16
# Scheme Name: dirtysea
# Scheme Author: Kahlil (Kal) Hodgson
# Template Author: Bez Hermoso

# Configures difftool "delta" to ue light or dark mode based on the variant declared by the colorscheme.
[delta]
    syntax-theme = "ansi"
    light = true
    minus-style = reverse "#840000"
    minus-emph-style = reverse "#000090"
    plus-style = reverse "#730073"
    plus-emph-style = reverse "#000090"
