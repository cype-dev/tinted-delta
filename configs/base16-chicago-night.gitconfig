# vim: ft=gitconfig
# Scheme System: base16
# Scheme Name: Chicago Night
# Scheme Author: Wendell, Ryan &lt;ryanjwendell@gmail.com&gt;
# Template Author: Bez Hermoso

# Configures difftool "delta" to ue light or dark mode based on the variant declared by the colorscheme.
[delta]
    syntax-theme = "ansi"
    light = false
    minus-style = reverse "#c60c30"
    minus-emph-style = reverse "#e27ea6"
    plus-style = reverse "#009b3a"
    plus-emph-style = reverse "#e27ea6"
