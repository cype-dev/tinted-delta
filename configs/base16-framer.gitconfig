# vim: ft=gitconfig
# Scheme System: base16
# Scheme Name: Framer
# Scheme Author: Framer (Maintained by Jesse Hoyos)
# Template Author: Bez Hermoso

# Configures difftool "delta" to ue light or dark mode based on the variant declared by the colorscheme.
[delta]
    syntax-theme = "ansi"
    light = false
    minus-style = reverse "#fd886b"
    minus-emph-style = reverse "#ba8cfc"
    plus-style = reverse "#32ccdc"
    plus-emph-style = reverse "#ba8cfc"
