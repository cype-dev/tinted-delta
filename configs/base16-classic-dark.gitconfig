# vim: ft=gitconfig
# Scheme System: base16
# Scheme Name: Classic Dark
# Scheme Author: Jason Heeris (http://heeris.id.au)
# Template Author: Bez Hermoso

# Configures difftool "delta" to ue light or dark mode based on the variant declared by the colorscheme.
[delta]
    syntax-theme = "ansi"
    light = false
    minus-style = reverse "#ac4142"
    minus-emph-style = reverse "#aa759f"
    plus-style = reverse "#90a959"
    plus-emph-style = reverse "#aa759f"
