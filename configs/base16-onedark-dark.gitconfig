# vim: ft=gitconfig
# Scheme System: base16
# Scheme Name: OneDark Dark
# Scheme Author: olimorris (https://github.com/olimorris)
# Template Author: Bez Hermoso

# Configures difftool "delta" to ue light or dark mode based on the variant declared by the colorscheme.
[delta]
    syntax-theme = "ansi"
    light = false
    minus-style = reverse "#ef596f"
    minus-emph-style = reverse "#d55fde"
    plus-style = reverse "#89ca78"
    plus-emph-style = reverse "#d55fde"
