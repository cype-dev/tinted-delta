# vim: ft=gitconfig
# Scheme System: base16
# Scheme Name: Primer Dark Dimmed
# Scheme Author: Jimmy Lin
# Template Author: Bez Hermoso

# Configures difftool "delta" to ue light or dark mode based on the variant declared by the colorscheme.
[delta]
    syntax-theme = "ansi"
    light = false
    minus-style = reverse "#f47067"
    minus-emph-style = reverse "#e275ad"
    plus-style = reverse "#57ab5a"
    plus-emph-style = reverse "#e275ad"
