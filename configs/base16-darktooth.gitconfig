# vim: ft=gitconfig
# Scheme System: base16
# Scheme Name: Darktooth
# Scheme Author: Jason Milkins (https://github.com/jasonm23)
# Template Author: Bez Hermoso

# Configures difftool "delta" to ue light or dark mode based on the variant declared by the colorscheme.
[delta]
    syntax-theme = "ansi"
    light = false
    minus-style = reverse "#fb543f"
    minus-emph-style = reverse "#8f4673"
    plus-style = reverse "#95c085"
    plus-emph-style = reverse "#8f4673"
