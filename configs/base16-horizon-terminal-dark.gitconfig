# Configures difftool "delta" to ue light or dark mode based on the variant declared by the colorscheme.
[delta]
    syntax-theme = "ansi"
    light = false
    minus-style = reverse "#e95678"
    minus-emph-style = reverse "#ee64ac"
    plus-style = reverse "#29d398"
    plus-emph-style = reverse "#ee64ac"
