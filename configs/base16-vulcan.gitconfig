# vim: ft=gitconfig
# Scheme System: base16
# Scheme Name: vulcan
# Scheme Author: Andrey Varfolomeev
# Template Author: Bez Hermoso

# Configures difftool "delta" to ue light or dark mode based on the variant declared by the colorscheme.
[delta]
    syntax-theme = "ansi"
    light = false
    minus-style = reverse "#818591"
    minus-emph-style = reverse "#9198a3"
    plus-style = reverse "#977d7c"
    plus-emph-style = reverse "#9198a3"
