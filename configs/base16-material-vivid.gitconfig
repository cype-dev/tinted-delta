# vim: ft=gitconfig
# Scheme System: base16
# Scheme Name: Material Vivid
# Scheme Author: joshyrobot
# Template Author: Bez Hermoso

# Configures difftool "delta" to ue light or dark mode based on the variant declared by the colorscheme.
[delta]
    syntax-theme = "ansi"
    light = false
    minus-style = reverse "#f44336"
    minus-emph-style = reverse "#673ab7"
    plus-style = reverse "#00e676"
    plus-emph-style = reverse "#673ab7"
