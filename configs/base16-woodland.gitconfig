# vim: ft=gitconfig
# Scheme System: base16
# Scheme Name: Woodland
# Scheme Author: Jay Cornwall (https://jcornwall.com)
# Template Author: Bez Hermoso

# Configures difftool "delta" to ue light or dark mode based on the variant declared by the colorscheme.
[delta]
    syntax-theme = "ansi"
    light = false
    minus-style = reverse "#d35c5c"
    minus-emph-style = reverse "#bb90e2"
    plus-style = reverse "#b7ba53"
    plus-emph-style = reverse "#bb90e2"
