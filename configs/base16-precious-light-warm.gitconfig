# vim: ft=gitconfig
# Scheme System: base16
# Scheme Name: Precious Light Warm
# Scheme Author: 4lex4 &lt;4lex49@zoho.com&gt;
# Template Author: Bez Hermoso

# Configures difftool "delta" to ue light or dark mode based on the variant declared by the colorscheme.
[delta]
    syntax-theme = "ansi"
    light = true
    minus-style = reverse "#b14745"
    minus-emph-style = reverse "#7a50c6"
    plus-style = reverse "#557300"
    plus-emph-style = reverse "#7a50c6"
