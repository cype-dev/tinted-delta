# vim: ft=gitconfig
# Scheme System: base16
# Scheme Name: Google Dark
# Scheme Author: Seth Wright (http://sethawright.com)
# Template Author: Bez Hermoso

# Configures difftool "delta" to ue light or dark mode based on the variant declared by the colorscheme.
[delta]
    syntax-theme = "ansi"
    light = false
    minus-style = reverse "#cc342b"
    minus-emph-style = reverse "#a36ac7"
    plus-style = reverse "#198844"
    plus-emph-style = reverse "#a36ac7"
