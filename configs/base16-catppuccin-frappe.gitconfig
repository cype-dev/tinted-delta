# vim: ft=gitconfig
# Scheme System: base16
# Scheme Name: Catppuccin Frappe
# Scheme Author: https://github.com/catppuccin/catppuccin
# Template Author: Bez Hermoso

# Configures difftool "delta" to ue light or dark mode based on the variant declared by the colorscheme.
[delta]
    syntax-theme = "ansi"
    light = false
    minus-style = reverse "#e78284"
    minus-emph-style = reverse "#ca9ee6"
    plus-style = reverse "#a6d189"
    plus-emph-style = reverse "#ca9ee6"
