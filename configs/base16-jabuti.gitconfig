# vim: ft=gitconfig
# Scheme System: base16
# Scheme Name: Jabuti
# Scheme Author: https://github.com/notusknot
# Template Author: Bez Hermoso

# Configures difftool "delta" to ue light or dark mode based on the variant declared by the colorscheme.
[delta]
    syntax-theme = "ansi"
    light = false
    minus-style = reverse "#ec6a88"
    minus-emph-style = reverse "#be95ff"
    plus-style = reverse "#3fdaa4"
    plus-emph-style = reverse "#be95ff"
