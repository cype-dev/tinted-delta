# vim: ft=gitconfig
# Scheme System: base16
# Scheme Name: Seti UI
# Scheme Author: 
# Template Author: Bez Hermoso

# Configures difftool "delta" to ue light or dark mode based on the variant declared by the colorscheme.
[delta]
    syntax-theme = "ansi"
    light = false
    minus-style = reverse "#cd3f45"
    minus-emph-style = reverse "#a074c4"
    plus-style = reverse "#9fca56"
    plus-emph-style = reverse "#a074c4"
