# vim: ft=gitconfig
# Scheme System: base16
# Scheme Name: lime
# Scheme Author: limelier
# Template Author: Bez Hermoso

# Configures difftool "delta" to ue light or dark mode based on the variant declared by the colorscheme.
[delta]
    syntax-theme = "ansi"
    light = false
    minus-style = reverse "#ff662a"
    minus-emph-style = reverse "#1b825f"
    plus-style = reverse "#8cd97c"
    plus-emph-style = reverse "#1b825f"
