# Configures difftool "delta" to ue light or dark mode based on the variant declared by the colorscheme.
[delta]
    syntax-theme = "ansi"
    light = false
    minus-style = reverse "#8c4665"
    minus-emph-style = reverse "#5e468c"
    plus-style = reverse "#287373"
    plus-emph-style = reverse "#5e468c"
