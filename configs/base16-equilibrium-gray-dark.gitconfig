# vim: ft=gitconfig
# Scheme System: base16
# Scheme Name: Equilibrium Gray Dark
# Scheme Author: Carlo Abelli
# Template Author: Bez Hermoso

# Configures difftool "delta" to ue light or dark mode based on the variant declared by the colorscheme.
[delta]
    syntax-theme = "ansi"
    light = false
    minus-style = reverse "#f04339"
    minus-emph-style = reverse "#6a7fd2"
    plus-style = reverse "#7f8b00"
    plus-emph-style = reverse "#6a7fd2"
