# Configures difftool "delta" to ue light or dark mode based on the variant declared by the colorscheme.
[delta]
    syntax-theme = "ansi"
    light = true
    minus-style = reverse "#8c4351"
    minus-emph-style = reverse "#5a4a78"
    plus-style = reverse "#33635c"
    plus-emph-style = reverse "#5a4a78"
