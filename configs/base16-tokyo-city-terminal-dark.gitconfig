# Configures difftool "delta" to ue light or dark mode based on the variant declared by the colorscheme.
[delta]
    syntax-theme = "ansi"
    light = false
    minus-style = reverse "#d95468"
    minus-emph-style = reverse "#b62d65"
    plus-style = reverse "#8bd49c"
    plus-emph-style = reverse "#b62d65"
