# Configures difftool "delta" to ue light or dark mode based on the variant declared by the colorscheme.
[delta]
    syntax-theme = "ansi"
    light = false
    minus-style = reverse "#e27373"
    minus-emph-style = reverse "#e1c0fa"
    plus-style = reverse "#93b979"
    plus-emph-style = reverse "#e1c0fa"
