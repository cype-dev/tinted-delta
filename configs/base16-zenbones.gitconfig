# vim: ft=gitconfig
# Scheme System: base16
# Scheme Name: Zenbones
# Scheme Author: mcchrish
# Template Author: Bez Hermoso

# Configures difftool "delta" to ue light or dark mode based on the variant declared by the colorscheme.
[delta]
    syntax-theme = "ansi"
    light = false
    minus-style = reverse "#3d3839"
    minus-emph-style = reverse "#65b8c1"
    plus-style = reverse "#d68c67"
    plus-emph-style = reverse "#65b8c1"
