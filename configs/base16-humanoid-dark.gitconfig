# vim: ft=gitconfig
# Scheme System: base16
# Scheme Name: Humanoid dark
# Scheme Author: Thomas (tasmo) Friese
# Template Author: Bez Hermoso

# Configures difftool "delta" to ue light or dark mode based on the variant declared by the colorscheme.
[delta]
    syntax-theme = "ansi"
    light = false
    minus-style = reverse "#f11235"
    minus-emph-style = reverse "#f15ee3"
    plus-style = reverse "#02d849"
    plus-emph-style = reverse "#f15ee3"
