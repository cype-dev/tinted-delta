# Configures difftool "delta" to ue light or dark mode based on the variant declared by the colorscheme.
[delta]
    syntax-theme = "ansi"
    light = false
    minus-style = reverse "#c0caf5"
    minus-emph-style = reverse "#bb9af7"
    plus-style = reverse "#9ece6a"
    plus-emph-style = reverse "#bb9af7"
