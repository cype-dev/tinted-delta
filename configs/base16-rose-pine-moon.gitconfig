# Configures difftool "delta" to ue light or dark mode based on the variant declared by the colorscheme.
[delta]
    syntax-theme = "ansi"
    light = false
    minus-style = reverse "#eb6f92"
    minus-emph-style = reverse "#f6c177"
    plus-style = reverse "#3e8fb0"
    plus-emph-style = reverse "#f6c177"
