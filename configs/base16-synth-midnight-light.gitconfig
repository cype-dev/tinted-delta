# vim: ft=gitconfig
# Scheme System: base16
# Scheme Name: Synth Midnight Terminal Light
# Scheme Author: MichaÃ«l Ball (http://github.com/michael-ball/)
# Template Author: Bez Hermoso

# Configures difftool "delta" to ue light or dark mode based on the variant declared by the colorscheme.
[delta]
    syntax-theme = "ansi"
    light = true
    minus-style = reverse "#b53b50"
    minus-emph-style = reverse "#ea5ce2"
    plus-style = reverse "#06ea61"
    plus-emph-style = reverse "#ea5ce2"
