# vim: ft=gitconfig
# Scheme System: base16
# Scheme Name: Primer Dark
# Scheme Author: Jimmy Lin
# Template Author: Bez Hermoso

# Configures difftool "delta" to ue light or dark mode based on the variant declared by the colorscheme.
[delta]
    syntax-theme = "ansi"
    light = false
    minus-style = reverse "#ff7b72"
    minus-emph-style = reverse "#f778ba"
    plus-style = reverse "#3fb950"
    plus-emph-style = reverse "#f778ba"
