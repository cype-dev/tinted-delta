# vim: ft=gitconfig
# Scheme System: base16
# Scheme Name: Ayu Dark
# Scheme Author: Khue Nguyen &lt;Z5483Y@gmail.com&gt;
# Template Author: Bez Hermoso

# Configures difftool "delta" to ue light or dark mode based on the variant declared by the colorscheme.
[delta]
    syntax-theme = "ansi"
    light = false
    minus-style = reverse "#f07178"
    minus-emph-style = reverse "#d2a6ff"
    plus-style = reverse "#b8cc52"
    plus-emph-style = reverse "#d2a6ff"
